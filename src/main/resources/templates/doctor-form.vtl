<form action="/doctors" method="post">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name" id="name">
  </div>
  #if ($specialities)
    <select name="specialities">
      #foreach ($speciality in $specialities)
        <option value="$speciality.getId()">$speciality.getName()</option>
      #end
    </select>
    <button type="submit" class="btn btn-danger">Add Doctor</button>
  #end
</form>
<a href="/specialities/new">Add Speciality</a>
